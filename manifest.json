{
  "manifest_version": 3,
  "name": "Bookmark GPT",
  "version": "1.3",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/Frame 128.png",
      "512": "images/Frame 512.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "images/Frame 16.png",
    "32": "images/Frame 32.png",
    "48": "images/Frame 48.png",
    "128": "images/Frame 128.png",
    "512": "images/Frame 512.png"
  }
}
